
theme = Oxocarbon

# custom fonts

# hide top bar
macos-titlebar-style = hidden
background = 282c34

# these are used by fzf in vim
keybind = ctrl+shift+v=ignore
keybind = ctrl+shift+s=ignore
keybind = ctrl+shift+t=ignore

keybind = alt+shift+p=ignore

#fix nvim c-o
keybind = ctrl+o=text:\x0f

# turn full screen off
keybind = cmd+enter=ignore

font-size =16.0

background-opacity = 0.9
copy-on-select = clipboard

